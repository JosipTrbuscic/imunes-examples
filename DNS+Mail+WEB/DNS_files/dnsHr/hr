; Tu cu ubaciti SOA
;
$TTL 60000
@ IN SOA dnsHr.hr. root.dnsHr.hr (
        2002102801 ; serial
        28 ; refresh
        14 ; retry
        3600000 ; expire
        0 ; default_ttl
)

@                   IN  NS   dnsHr.hr.
@                   IN  NS   hr2.com.
dnsHr.hr.           IN  A    10.0.6.2
hr2.com.            IN  A    10.0.2.1

fer.hr.             IN  NS   dnsFer.fer.hr.
dnsFer.fer.hr.      IN  A    10.0.8.2

